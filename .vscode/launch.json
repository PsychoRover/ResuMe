    "version": "0.2.0",
    "configurations": [
        {
            "name": "Main",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main_service.main:app",
                "--reload",
                "--port",
                "8080"
            ],
            "jinja": true,
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Model",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.model_service.model:app",
                "--reload",
                "--port",
                "8000"
            ],
            "jinja": true,
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
    ],
}